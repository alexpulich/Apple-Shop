footer.footer
		div.wrap
			div.logo-mono
				a(href="/").logo-mono__link
					| ЭпплShop
			nav.subnav
				-var subnavhead = ['О магазине', 'Каталог', 'Контакты'];
				-var subnavabout = {'О нас': '', 'Наши цены': '', 'Наши скидки': '', 'Акции': ''};
				-var subnavcatalog = {'Смартфоны': '', 'Планшеты': '', 'Компьютеры': '', 'Аксессуары': ''};
				-var subnavcontacts = {'Телефоны': '', 'Адреса': '', 'Карта проезда': '', 'Обратная связь': ''};
				each val in subnavhead
					div.subnav__column
						div.subnav__heading=val
						ul.subnav__list
							if val === 'О магазине'
								each key, value in subnavabout
									li.subnav__item
										a.subnav__link(href=key)=value
							else if val === 'Каталог'
								each key, value in subnavcatalog
									li.subnav__item
										a.subnav__link(href=key)=value
							else if val === 'Контакты'
								each key, value in subnavcontacts
									li.subnav__item
										a.subnav__link(href=key)=value
			div.subsocials
				ul.subsocials__apps-list
					li.subsocials__item
						a.subsocials__link.appstore(href='')
							| Доступно в AppStore
					li.subsocials__item
						a.subsocials__link.googleplay(href='')
							| Доступно в Google Play
				ul.subsocials__socials-list
					li.subsocials__item
						a.subsocials__link.vk(href='vk.com/ablend')
							| ВКонтакте
					li.subsocials__item
						a.subsocials__link.fb(href='fb.com')
							| Facebook
					li.subsocials__item
						a.subsocials__link.tw(href='twitter.com')
							| Twitter
